@model TeamMemberListViewModel

<h1>Team Members</h1>

@foreach (var teamMember in Model.TeamMembers)
{
    <div class="col-sm-4 col-lg-4 col-md-4">
        <div class="thumbnail">
            <div class="caption">
                <h3 class="pull-right">@teamMember.FirstName</h3>
                <h3 class="pull-right">@teamMember.LastName</h3>
                <p>@teamMember.Role</p>
            </div>
        </div>
    </div>
}

<p class="button">
    <a class="btn btn-primary" asp-controller="TeamMember" asp-action="Add">Add a Team Member</a>
</p>